.container
  padding 8px
  box-shadow 2px 2px 5px rgba(0,0,0,0.4)
  margin-bottom 15px
.row
  margin-left -8px
  margin-right -8px
  display flex
  margin-top 8px
  margin-bottom 8px
  &>div
    padding 0 8px
  &>div:nth-child(1)
    width 20%
    font-weight bold
    text-align right
  &>div:nth-child(2)
    width 80%
.date-container
  display flex
  &>div
    margin-right 8px
.list>li
  list-style disc
  margin-left 20px

